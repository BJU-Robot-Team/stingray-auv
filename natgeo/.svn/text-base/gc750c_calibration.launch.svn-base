<launch>
    <arg name="start_stereo" default="false"/>

    <include file="$(find natgeo)/gc750c.launch">
        <arg name="start_stereo" value="$(arg start_stereo)"/>
    </include>

    <node pkg="camera_calibration" type="cameracalibrator.py" name="stereo_calibration"
          args="--size 6x5 --square 0.04 right:=/stereo/right/image_raw left:=/stereo/left/image_raw right_camera:=/stereo/right left_camera:=/stereo/left --approximate=0.005">
    </node>
</launch>
